{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "app/dist": true,
    "node_modules": true,
    "package-lock.json": true,
    "tmp": true
  },
  "eslint.run": "onType",
  "eslint.validate": [
    "javascript",
    "javascriptreact"
  ],
  "prettier.eslintIntegration": false,
  "prettier.ignorePath": ".prettierignore",
  "eslint.autoFixOnSave": true,
  "eslint.enable": true
}